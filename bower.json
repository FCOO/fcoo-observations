{
  "name": "fcoo-observations",
  "version": "1.0.1",
  "homepage": "https://github.com/FCOO/fcoo-observations",
  "authors": [
    "Niels Holt"
  ],
  "description": "",
  "main": [
    "dist/fcoo-observations.js",
    "dist/fcoo-observations.css"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/FCOO/fcoo-observations.git"
  },
  "dependencies": {
    "jquery": "latest",
    "leaflet": "latest",
    "promise-get": "fcoo/promise-get#^2.0.0",
    "fcoo-data-files": "fcoo/fcoo-data-files#latest",
    "leaflet-bootstrap-marker": "fcoo/leaflet-bootstrap-marker#^2.3.2",
    "leaflet-bootstrap": "fcoo/leaflet-bootstrap#^2.7.6",
    "fcoo-value-format": "fcoo/fcoo-value-format#^3.0.0",
    "fcoo-fontawesome": "https://gitlab.com/FCOO/fcoo-fontawesome.git#^5.7.2"
  },
  "devDependencies": {
    "bourbon": "latest",
    "modernizr-mixin": "latest",
    "mathsass": "latest"
  },
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "moduleType": [
    "es6"
  ],
  "overrides": {
    "bootstrap-select": {
      "main": [
        "dist/js/bootstrap-select.js"
      ]
    },
    "jquery.scrollbar": {
      "main": [
        "jquery.scrollbar.js"
      ]
    },
    "bootstrap": {
      "dependencies": {
        "jquery": "3.4.1"
      },
      "main": [
        "dist/js/bootstrap.bundle.js"
      ]
    },
    "inputmask": {
      "main": [
        "dist/jquery.inputmask.bundle.js"
      ]
    },
    "js-url": {
      "main": [
        "dist/url.min.js"
      ]
    },
    "perfect-scrollbar": {
      "main": [
        "dist/perfect-scrollbar.js",
        "css/perfect-scrollbar.css"
      ]
    },
    "leaflet": {
      "main": [
        "dist/leaflet-src.js",
        "dist/leaflet.css"
      ],
      "dependencies": {
        "leaflet-global-switches": "latest"
      }
    },
    "moment": {
      "main": [
        "moment.js",
        "locale/da.js",
        "locale/de.js",
        "locale/sv.js",
        "locale/nb.js",
        "locale/fo.js"
      ]
    }
  },
  "resolutions": {
    "bootstrap": "^4.0.0",
    "jquery": "~3.4.0"
  }
}
